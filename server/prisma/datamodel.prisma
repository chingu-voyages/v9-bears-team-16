type Theme {
  id: ID! @id
  title: String!
  description: String!
  colors: ColorPallete!
  fonts: FontPairing!
}

type ColorPallete {
  id: ID! @id
  title: String
  primaryLight: String!
  primaryDark: String!
  accentLight: String!
  accentDark: String!
  accentBrand: String!
}

type FontPairing {
  id: ID! @id
  title: String
  primary: String!
  secondary: String!
}
